<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_drawer"
    app:startDestination="@id/dashboardFragment">

    <fragment
        android:id="@+id/dashboardFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.DashboardFragment"
        android:label="fragment_dashboard"
        tools:layout="@layout/fragment_dashboard" >
        <action
            android:id="@+id/action_dashboardFragment_to_listOrderFragment"
            app:destination="@id/listOrderFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_akunFragment"
            app:destination="@id/akunFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_sopirFragment"
            app:destination="@id/sopirFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_armadaFragment"
            app:destination="@id/armadaFragment" />
        <action
            android:id="@+id/action_dashboardFragment_to_restoFragment"
            app:destination="@id/restoFragment" />
    </fragment>
    <fragment
        android:id="@+id/akunFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.AkunFragment"
        android:label="fragment_akun"
        tools:layout="@layout/fragment_akun" />
    <fragment
        android:id="@+id/listOrderFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.ListOrderFragment"
        android:label="ListOrderFragment"
        tools:layout="@layout/fragment_list_order" />
    <fragment
        android:id="@+id/sopirFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.SopirFragment"
        android:label="fragment_sopir"
        tools:layout="@layout/fragment_sopir" />
    <fragment
        android:id="@+id/armadaFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.ArmadaFragment"
        android:label="fragment_armada"
        tools:layout="@layout/fragment_armada" />
    <fragment
        android:id="@+id/restoFragment"
        android:name="com.baharudin.traveladmin.ui.fragment.RestoFragment"
        android:label="fragment_resto"
        tools:layout="@layout/fragment_resto" />
</navigation>